.title-img image{
    width:40px;
    height:40px
}
.list navigator{
    display: flex;
    flex-direction: row;
    align-items: center;
    line-height: 20px;
    height:70px;
    padding:8px 18px 8px;
    border-bottom: 1px solid #ccc;
}
.list>navigator .title-img{
    padding-right: 10px;
}
.list:active{
    background-color:#e5e5e5;
    transition: background-color 0.5s ease;
}